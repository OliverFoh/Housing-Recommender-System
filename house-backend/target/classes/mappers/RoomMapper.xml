  <select id="getRoomByConditions"  resultMap="BaseResultMap" >
    select id,title,city,district,price,ori,room_type,house_type,rental_method,pic_url from room where city=#{city}
      <if test="rentalMethod!='null'">
        and rental_method=#{rentalMethod}
      </if>
    <if test="decorateType!='null'">
        and decorate_type=#{decorateType}
    </if>
    <if test="houseType!='null'">
      and house_type=#{houseType}
    </if>
    <if test="ori!='null'">
      and ori=#{ori}
    </if>
      <if test="sortType!='null'">
          order by price ${sortType}
      </if>
  </select>
